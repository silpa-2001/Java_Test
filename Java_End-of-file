import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s= new Scanner(System.in);
        int i=1;
        String str;
        while(s.hasNextLine())
        {
            str=s.nextLine();
            System.out.println(i+" "+str);
            i++;
        }
    }
}
